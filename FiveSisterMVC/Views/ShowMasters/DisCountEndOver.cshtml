<script>
    $(function () {
        ShowDisCount();
    })
    function ShowDisCount() {
        $.ajax({
            url: "http://localhost:50519/api/discount/GetEndDisCountInfosById?UserId=2",
            dataType: "json",
            type: "get",
            contentType: "application/x-www-form-urlencoded",
            success: function (d) {
                console.log(d);
                $("#hezi").empty();
                $(d).each(function () {
                    var htm = "<div style='float:left;width:200px;height:248px;margin-top:10px;margin-left:38px;'>" +
                        " <div class='coupon-item coupon-item-d'>" +
                        "<div class='coupon-list'>" +
                        "<div class='c-type'>" +
                        "<div class='c-class'> <strong>购物券</strong></div>" +
                        "<div class='c-price'> <strong>￥" + this.disCountInfoPrice + "</strong> </div>" +
                        "<div class='c-limit'> 【无门槛使用】 </div>" +
                        "<div class='c-time'><span>使用期限</span>" + this.showStartTime + "&nbsp;--&nbsp;" + this.showEndTime + "</div>" +
                        "<div class='c-type-top'></div>" +
                        "<div class='c-type-bottom'></div></div><div style='background-color:#EEEEEE;' class='c-msg'>" +
                        "<div class='c-range'>" +
                        "<div class='range-all'>" +
                        "<div class='range-item'>" +
                        "<span class='label'></span>" +
                        "<span class='txt'></span>" +
                        "</div></div></div>" +
                        "<div class='op-btns'>" +
                        "<a href='javascript:DeleDis(" + this.disCountAndUserId + ")' class='btn'><span class='txt'>立即删除</span>" +
                        "<b></b></a>" +
                        "</div></div></div></div></div>";
                    $("#hezi").append(htm);
                })
            }
        });
    }
    //删除优惠券
    function DeleDis(id) {
        $.ajax({
            url: "http://localhost:50519/api/discount/DeleteDisCount?DisAndUserId=" + id,
            dataType: "json",
            type: "get",
            contentType: "application/x-www-form-urlencoded",
            success: function (d) {
                if (d > 0) {
                    layer.msg('删除成功!', { time: 1000 });
                    setTimeout(function () {
                        ShowDisCount();
                    }, 1000);
                }
            }
        });
    }
</script>
